          RowBox[{"PlotRange", "[", "plot", "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"scale", "=", 
        RowBox[{
         RowBox[{"Abs", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"PlotRange", "[", "plot", "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "2"}], "]"}], "]"}], "-", 
           RowBox[{
            RowBox[{"PlotRange", "[", "plot", "]"}], "[", 
            RowBox[{"[", 
