           RowBox[{"0", ",", "h"}], "}"}]}], "}"}]}], ",", 
       RowBox[{"ImageSize", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"w", ",", "h"}], "}"}]}], ",", 
