Cell[BoxData[
 RowBox[{
  RowBox[{"\[Theta]PreProcess", "[", 
   RowBox[{"table_", ",", 
    RowBox[{"yscale_:", "1"}]}], 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"optional", " ", "scale", " ", "on", " ", "right", " ", "y", " ", 
      RowBox[{"axis", ":", " ", 
       RowBox[{"if", " ", "1"}]}]}], ",", " ", 
     RowBox[{"then", " ", "no", " ", "numbers"}], ",", " ", 
     RowBox[{"only", " ", "ticks"}]}], "*)"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"plot", "=", 
         RowBox[{"table", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "j"}], "]"}], "]"}]}], ",", "scale", ",", 
        "fticks", ",", "gticks"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fticks", "=", 
