Cell[CellGroupData[{

Cell["\<\
Define function to align several graphics into same sizes in a column: \
alignedGraphicsColumn[list,opts]\
\>", "Subsection",
 CellChangeTimes->{
  3.708191973628079*^9, {3.708265475794119*^9, 
   3.708265540970641*^9}},ExpressionUUID->"ca081a19-39b4-4433-94d8-\
ef36eea10d78"],

Cell[TextData[{
 StyleBox["Explain Options:",
  FontWeight->"Bold"],
 "\n Accepts Graphics options for the whole image.\n\nIn the current \
implementation, ImageSize is broken, but it's functionality is mimicked with:\
\n\n",
 StyleBox["SubImageWidth",
  FontWeight->"Bold"],
 ": an option that determines the figure width.\n        Choosing a positive \
real number x mimics the behavior of ImageSize -> x.\n        Full scales all \
of the sub-plots so that they all have the width of the sub-plot with the \
largest width, and the ImageSize is then this width.\n        Automatic is \
the default: the ImageSize is the width of the sub-plot with the largest \
width.\n\n",
 StyleBox["FrameAligned",
  FontWeight->"Bold"],
 " determines the alignment functionality.\n        All: all plots get the \
same ImagePadding, and so the plots get lined up nicely, with lots of space \
between them, which can be fixed with Spacings.\n        LeftOnly: all plots \
get the same ImagePadding on the left and the right, which aligns the left \
sides of the frames only, unless all sub-plots have the same ImageSize.\n     \
   None: applies no ImagePadding, so the plots do not line up.\n\n",
 StyleBox["Spacings",
  FontWeight->"Bold"],
 ": works as advertised in GraphicsGrid and related functions."
}], "Text",
 CellChangeTimes->{{3.7081920329316683`*^9, 3.70819205297598*^9}, {
  3.708265363529519*^9, 3.708265393100412*^9}, {3.708265423613845*^9, 
  3.708265448909401*^9}, {3.708274494495669*^9, 
  3.708274495441613*^9}},ExpressionUUID->"10e11eb8-cbaa-4de2-8770-\
b554c3a4038d"],

Cell[CellGroupData[{

Cell["\<\
Define function alignedGraphicsColumn[list_,opts:OptionsPattern[]]\
\>", "Subsubsection",
 CellChangeTimes->{{3.708265223621358*^9, 
  3.7082652820071793`*^9}},ExpressionUUID->"d8a2dc65-c4bc-454c-a069-\
d4c2116bc60c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "alignedGraphicsColumn", "]"}], "\n", 
  RowBox[{"(*", "options", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Options", "[", "alignedGraphicsColumn", "]"}], "=", 
    RowBox[{"Join", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Spacings", "\[Rule]", "0"}], ",", 
        RowBox[{"FrameAligned", "\[Rule]", "LeftOnly"}], ",", 
        RowBox[{"SubImageWidth", "\[Rule]", "Automatic"}]}], "}"}], ",", 
      RowBox[{"Options", "[", "Graphics", "]"}]}], "]"}]}], ";"}], "\n", 
  RowBox[{"(*", 
   RowBox[{"error", " ", "message", " ", "texts"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"alignedGraphicsColumn", "::", "faligned"}], "=", 
   "\"\<Value of option FrameAligned is not All, None, or LeftRight.\>\""}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"alignedGraphicsColumn", "::", "width"}], "=", 
    "\"\<Value of option SubImageWidth is not Full, Automatic, or a positive \
machine number.\>\""}], ";"}], "\n", 
  RowBox[{"(*", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"alignedGraphicsColumn", "[", 
   RowBox[{"list_", ",", 
    RowBox[{"opts", ":", 
     RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sizes", ",", "width", ",", "plots", ",", 
      RowBox[{"optWidth", "=", 
       RowBox[{"OptionValue", "[", "SubImageWidth", "]"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{"And", "[", 
        RowBox[{
         RowBox[{"optWidth", "=!=", "Automatic"}], ",", 
         RowBox[{"optWidth", "=!=", "Full"}], ",", 
         RowBox[{"!", 
          RowBox[{"NumericQ", "@", "optWidth"}]}]}], "]"}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"Message", "[", 
         RowBox[{"alignedGraphicsColumn", "::", "width"}], "]"}], "]"}], ",", 
       
       RowBox[{"optWidth", "===", "Automatic"}], ",", 
       RowBox[{"plots", "=", "list"}], ",", 
       RowBox[{"optWidth", "===", "Full"}], ",", 
       RowBox[{"plots", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Show", "[", 
           RowBox[{"#", ",", 
            RowBox[{"ImageSize", "\[Rule]", 
             RowBox[{"Max", "@", 
              RowBox[{"First", "@", 
               RowBox[{"(", 
                RowBox[{"ImageDimensions", "/@", "list"}], ")"}]}]}]}]}], 
           "]"}], "&"}], "/@", "list"}]}], ",", 
       RowBox[{"NumericQ", "@", "optWidth"}], ",", 
       RowBox[{"Which", "[", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"Element", "[", 
           RowBox[{"optWidth", ",", "Reals"}], "]"}]}], ",", 
         RowBox[{"Return", "[", 
          RowBox[{"Message", "[", 
           RowBox[{"alignedGraphicsColumn", "::", "width"}], "]"}], "]"}], 
         ",", 
         RowBox[{"optWidth", "\[LessEqual]", "0"}], ",", 
         RowBox[{"Return", "[", 
          RowBox[{"Message", "[", 
           RowBox[{"alignedGraphicsColumn", "::", "width"}], "]"}], "]"}], 
         ",", 
         RowBox[{"optWidth", ">", "0"}], ",", 
         RowBox[{"plots", "=", 
          RowBox[{
           RowBox[{
            RowBox[{"Show", "[", 
             RowBox[{"#", ",", 
              RowBox[{"ImageSize", "\[Rule]", "optWidth"}]}], "]"}], "&"}], "/@",
            "list"}]}]}], "]"}], ",", "True", ",", 
       RowBox[{"plots", "=", "list"}]}], "]"}], ";", 
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{"And", "@@", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"OptionValue", "[", "FrameAligned", "]"}], "=!=", "#"}], 
           "&"}], "/@", 
          RowBox[{"{", 
           RowBox[{"LeftOnly", ",", "All", ",", "None"}], "}"}]}], ")"}]}], 
       ",", 
       RowBox[{"Return", "[", 
        RowBox[{"Message", "[", 
         RowBox[{"alignedGraphicsColumn", "::", "faligned"}], "]"}], "]"}], 
       ",", 
       RowBox[{
        RowBox[{"OptionValue", "[", "FrameAligned", "]"}], "===", 
        "LeftOnly"}], ",", 
       RowBox[{"plots", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Show", "[", 
           RowBox[{"#", ",", 
            RowBox[{"ImagePadding", "\[Rule]", 
             RowBox[{"leftRightPadding", "@@", "plots"}]}]}], "]"}], "&"}], "/@",
          "plots"}]}], ",", 
       RowBox[{
        RowBox[{"OptionValue", "[", "FrameAligned", "]"}], "===", "All"}], 
       ",", 
       RowBox[{"plots", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"Show", "[", 
           RowBox[{"#", ",", 
            RowBox[{"ImagePadding", "\[Rule]", 
             RowBox[{"maxPadding", "@@", "plots"}]}]}], "]"}], "&"}], "/@", 
         "plots"}]}], ",", 
       RowBox[{
        RowBox[{"OptionValue", "[", "FrameAligned", "]"}], "===", "None"}], 
       ",", "True"}], "]"}], ";", 
     RowBox[{"sizes", "=", 
      RowBox[{"ImageDimensions", "/@", "plots"}]}], ";", 
     RowBox[{"width", "=", 
      RowBox[{"Max", "@", 
       RowBox[{"sizes", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}]}]}], ";", 
     RowBox[{"sizes", "=", 
      RowBox[{
       RowBox[{"sizes", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "2"}], "]"}], "]"}], "+", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "0", "}"}], ",", 
         RowBox[{"ConstantArray", "[", 
          RowBox[{
           RowBox[{"OptionValue", "[", "Spacings", "]"}], ",", 
           RowBox[{
            RowBox[{"Length", "@", "plots"}], "-", "1"}]}], "]"}]}], 
        "]"}]}]}], ";", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Inset", "[", 
          RowBox[{
           RowBox[{"plots", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"-", 
              RowBox[{"Plus", "@@", 
               RowBox[{"sizes", "[", 
                RowBox[{"[", 
                 RowBox[{";;", "i"}], "]"}], "]"}]}]}]}], "}"}], ",", 
           RowBox[{"ImageScaled", "[", 
            RowBox[{"{", 
             RowBox[{"0", ",", "0"}], "}"}], "]"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"Length", "[", "plots", "]"}]}], "}"}]}], "]"}], ",", 
       RowBox[{"ImageSize", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"width", ",", 
          RowBox[{"Plus", "@@", "sizes"}]}], "}"}]}], ",", 
       RowBox[{"ImagePadding", "\[Rule]", "None"}], ",", 
       RowBox[{"PlotRange", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", "width"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Plus", "@@", "sizes"}]}], ",", "0"}], "}"}]}], "}"}]}], 
       ",", 
       RowBox[{"AspectRatio", "\[Rule]", 
        RowBox[{
         RowBox[{"Plus", "@@", "sizes"}], "/", "width"}]}], ",", 
       RowBox[{"PlotRangePadding", "\[Rule]", "None"}], ",", 
       RowBox[{"FilterRules", "[", 
        RowBox[{
         RowBox[{"{", "opts", "}"}], ",", 
         RowBox[{"Options", "[", "Graphics", "]"}]}], "]"}]}], "]"}]}]}], 
   "]"}]}]}], "Input",
 CellChangeTimes->{{3.7081919288354483`*^9, 3.7081919438498096`*^9}, {
  3.7082652921347322`*^9, 3.7082653481066217`*^9}},
 CellLabel->"In[83]:=",ExpressionUUID->"40e4ff42-2ad4-4397-9bd0-d2918178313d"]
